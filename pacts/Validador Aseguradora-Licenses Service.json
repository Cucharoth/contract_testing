{
  "consumer": {
    "name": "Validador Aseguradora"
  },
  "interactions": [
    {
      "description": "a request to verify a missing license",
      "providerStates": [
        {
          "name": "license L-404 does not exist"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/licenses/L-404/verify"
      },
      "response": {
        "body": {
          "valid": false
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 404
      }
    },
    {
      "description": "a request to verify an issued license",
      "providerStates": [
        {
          "name": "patient 11111111-1 has issued license folio L-1001"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/licenses/L-1001/verify"
      },
      "response": {
        "body": {
          "valid": true
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "Licenses Service"
  }
}